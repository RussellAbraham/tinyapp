<%- include('partials/_header') %>

    <main class="container-fluid p-2">
      <h3>My URLs</h3>
      <table class="table">
        <thead>
          <tr>
            <th scope="col">Short URL ID</th>
            <th scope="col">Long URL</th>
          </tr>
        </thead>
        <tbody>
          <% for (let id in urls) { %>
            <tr>
              <td><%= id %></td>
              <td><%= urls[id] %></td>
            </tr>
          <% } %>
        </tbody>
      </table>
    </main>
    
<%- include('partials/_footer') %>